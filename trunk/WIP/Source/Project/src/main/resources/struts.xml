<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC 
    "-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
    "http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
	<package name="myPackage" extends="struts-default,json-default">
		<action name="index" class="indexAction">
			<result>/jsp/index.jsp</result>
			<result name="error">/jsp/error.jsp</result>
		</action>
		<action name="testPaypalStart" class="successAction">
			<result>/jsp/testpaypal.jsp</result>
			<result name="error">/jsp/common/error.jsp</result>
		</action>
		<action name="testPaypalPay" class="testPayAction">
			<result>/jsp/testpaypal_redirect.jsp</result>
			<result name="error">/jsp/common/error.jsp</result>
		</action>
		<action name="testPaypalSuccess" class="testPaySuccessAction">
			<result>/jsp/testpaypal_success.jsp</result>
			<result name="error">/jsp/common/error.jsp</result>
		</action>
		<action name="testPaypalCancel" class="successAction">
			<result>/jsp/testpaypal_cancel.jsp</result>
			<result name="error">/jsp/common/error.jsp</result>
		</action>
		<action name="helloWorld" class="helloWorldAction">
			<result>/jsp/helloWorld.jsp</result>
			<result name="error">/jsp/error.jsp</result>
		</action>
		<action name="listEmployee" class="listEmployeeAction">
			<result>/jsp/employee.jsp</result>
			<result name="error">/jsp/error.jsp</result>
		</action>
	</package>
</struts>
